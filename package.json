{
  "name": "mtwe",
  "version": "0.3.0",
  "description": "Twitter in Electron",
  "main": "main.js",
  "build": {
    "productName": "mtwe",
    "appId": "mtwe",
    "mac": {
      "icon": "icons/mac/icon.icns"
    },
    "win": {
      "icon": "icons/win/icon.ico"
    }
  },
  "scripts": {
    "start": "electron .",
    "patch": "cpx patch/index.js node_modules/electron-context-menu",
    "pack": "cpx patch/index.js node_modules/electron-context-menu && electron-builder --dir"
  },
  "repository": "https://github.com/kondoumh/mtwe",
  "keywords": [
    "Electron",
    "Twitter"
  ],
  "author": "kondoumh",
  "license": "MIT",
  "devDependencies": {
    "cpx": "^1.5.0",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "braces": ">=2.3.1",
    "minimist": ">=1.2.2"
  },
  "dependencies": {
    "about-window": "^1.13.4",
    "electron-context-menu": "^2.0.1",
    "electron-search-text": "^0.3.0",
    "electron-store": "^5.2.0"
  }
}
